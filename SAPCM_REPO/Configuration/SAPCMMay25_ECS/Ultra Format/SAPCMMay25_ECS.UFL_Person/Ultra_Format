<exportmultiplex category="Ultra_Format" ref_path="ultra_format[Internal]/storable[ultra_format]/storable[Data]">
   <string name="Definition"><![CDATA[external personExt{
    ascii id : terminated_by(",");
    ascii name : terminated_by(",");
    ascii city : terminated_by(0xA);
};
internal personInt{
    long id;
    string name;
    string city;    
};

in_map personImap : external(personExt),internal(personInt){automatic;};

decoder personDec : in_map(personImap);

internal fileErrUdr{
    string filename; 
};

out_map personOmap : external(personExt),internal(personInt){automatic;};

encoder personEnc : out_map(personOmap);]]></string>
</exportmultiplex>
