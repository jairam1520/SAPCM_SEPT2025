<exportmultiplex category="Ultra_Format" ref_path="ultra_format[Internal]/storable[ultra_format]/storable[Data]">
   <string name="Definition"><![CDATA[/*
MSISDN,IMSI,ServiceType,AccountID,FileName,BatchDate,PartnerDomain,Duration
*/
internal usageOut{
	string MSISDN;
	string IMSI;
	string ServiceType;
	string AccountID;
	string FileName;
	string BatchDate;
	string PartnerDomain;
	int Duration;
};

external usageExt{
    ascii MSISDN : terminated_by(",");
    ascii IMSI : terminated_by(",");
    ascii ServiceType : terminated_by(",");
    ascii AccountID : terminated_by(",");
    ascii FileName : terminated_by(",");
    ascii BatchDate : terminated_by(",");
    ascii PartnerDomain : terminated_by(",");
    ascii Duration : terminated_by("\n");
};

out_map usgOMap : external(usageExt),internal(usageOut){automatic;};
encoder usgEnc : out_map(usgOMap);]]></string>
</exportmultiplex>
