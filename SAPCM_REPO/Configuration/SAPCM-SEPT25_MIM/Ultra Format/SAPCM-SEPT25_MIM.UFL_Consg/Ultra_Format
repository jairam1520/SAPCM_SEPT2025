<exportmultiplex category="Ultra_Format" ref_path="ultra_format[Internal]/storable[ultra_format]/storable[Data]">
   <string name="Definition"><![CDATA[external consgExt{
    ascii consgNumber : terminated_by(";");
    ascii consgType : terminated_by(";");   
    ascii custNum : terminated_by(";"); 
    ascii weight : terminated_by("\n");
};

in_map conImap : external(consgExt),target_internal(consgInt){automatic;};
decoder conDec : in_map(conImap);

out_map conOmap : external(consgExt),internal(consgInt){automatic;};
encoder conEnc : out_map(conOmap);


external consgErrExt{
    ascii consgNumber : terminated_by(";");
    ascii consgType : terminated_by(";");   
    ascii custNum : terminated_by(";"); 
    ascii weight : terminated_by(";");
    ascii ERR_MSG : terminated_by("\n");
}; 
in_map conErrImap : external(consgErrExt),target_internal(consgErrInt){automatic;};

out_map conErrOmap : external(consgErrExt),internal(consgErrInt){automatic;};
encoder conErrEnc : out_map(conErrOmap);
]]></string>
