string getCycleDate(){
    date start = dateCreateNow();   
    dateSetDay(start,1);
    
    date end = dateCreateCopy(start);
    dateAddMonths(end,1);
    dateAddDays(end,-1);
    
    return \"\"+ start +\" to \"+end;
