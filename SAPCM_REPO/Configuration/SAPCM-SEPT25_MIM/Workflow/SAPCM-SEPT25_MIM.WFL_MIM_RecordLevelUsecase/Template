                  <string name="sourceCode"><![CDATA[consume {
    pospaidTI post = (pospaidTI)input;
    
    postpaidIntermediate postInter = udrCreate(postpaidIntermediate);
    postInter.billingCycle = getCycleDate();
    postInter.msisdn = post.msisdn;
    postInter.planType = "GOLD_FREEDOM";
    double amt;
    strToDouble(amt,post.usage);
    postInter.taxAmount = (amt *18)/100;
    postInter.usage = (string)amt;
    debug("PostInter:"+postInter);
    udrRoute(postInter);
}

string getCycleDate(){
    date start = dateCreateNow();   
    dateSetDay(start,1);
    
    date end = dateCreateCopy(start);
    dateAddMonths(end,1);
    dateAddDays(end,-1);
    
    return ""+ start +" to "+end;
}]]></string>
