                  <string name="mainScript"><![CDATA[beginBatch{
     //flush the previous batch sessions
    if(strREContains((string)mimGet("Disk_1","Source Filename"),"blank")){
        debug("Flushing");
        aggregationHintFlushSessions("Aggregation_1");  
    }
     
}
sessionInit{     
   //triggered when record with new key is arrived
   //automatically session udr will be created
   //input reference is available
   utilityUsg utility = (utilityUsg)input;
   
   //session will timedout at the end of batch
   //sessionTimeout(session,-1);
   
   //session will timedout after 10 sec of creation
   sessionTimeout(session,10);
   
}
consume {
    //It will executed if session is created or existing session is matched
    //session ref is accessible here(new or existing matched)
    if(session.rec == null){
        //newly created session   
        session.rec = input; 
    }else{
        //existing session matched    
        //1. fetch the existing udr
        utilityUsg existingUtl = session.rec;
        //2. aggregate the usage amount, leave other fields as is
        double oldUsg,newUsg;
        strToDouble(oldUsg,existingUtl.Consumption_kWh);
        strToDouble(newUsg,input.Consumption_kWh);
        existingUtl.Consumption_kWh = (string)(oldUsg + newUsg);
    }
}
timeout{
    debug("Timeout");
    //It will be triggred for every session whose timeout as reached
    //session ref is  accessible(not new or matched but whose timeout expired)
    utilityUsg utlTimedoutRecord = session.rec;  
    udrRoute(utlTimedoutRecord);
    sessionRemove(session);//remove the sessions once aggregated and processed
}]]></string>
