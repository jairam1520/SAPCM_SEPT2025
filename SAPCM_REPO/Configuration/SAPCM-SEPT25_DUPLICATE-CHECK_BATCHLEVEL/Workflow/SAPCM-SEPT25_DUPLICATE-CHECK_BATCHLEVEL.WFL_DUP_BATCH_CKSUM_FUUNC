    tbCollAudit = tableCreate(DB_PRF,\"select * from COLL_ADT\");
}
consume {
    cksumUpdate(input);
    listAdd(inputList,input);
    isFileExist = true;
}

drain{
    debug(\"drain executed\");
    if(isFileExist == false){
        debug(\"No file exist\");
    }else{
        long ck = cksumResult();
        boolean isBatchDuplicate = checDupBatch(ck,tbCollAudit,(string)mimGet(\"Workflow\",\"Workflow Name\"));
        if(isBatchDuplicate == false){
            for(bytearray ipRec : inputList){
                udrRoute(ipRec);    
            }    
        }else{
            abort(\"Duplicate batch detected:\"+(string)mimGet(\"COLL_DIR\",\"Source Filename\"));    
        }
    }
}

