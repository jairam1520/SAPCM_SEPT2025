    mpAudit = mapCreate(string,any);
    duplicateCheckFlag = false;
    string wfName = getWfName((string)mimGet(\"Workflow\",\"Workflow Name\"));
    string serviceName = getService(wfName);
    mapSet(mpAudit,\"WF_NAME\",wfName);
