    duplicateCheckFlag = checkDupFlag(tbDupMasterRule,mpAudit);
    
    cksumReset();
}
consume {
    cksumUpdate(input);
    baInput = input;
}
drain{
    long checksum = cksumResult();
    mapSet(mpAudit,\"CHECKSUM\",checksum);
    if(duplicateCheckFlag == true){
        table tbCollAdt = sqlPrepSelect(collAdtQuery ,checksum,(string)mapGet(mpAudit,\"WF_NAME\"),DB);
        debug(\"tbCollAdt:\"+tbCollAdt);
        if(tableRowCount(tbCollAdt) == 0){
            udrRoute(baInput);
        } else{
            abort(\"Duplicate batch detected\");    
        }   
    } else{
        udrRoute(baInput) ;   
    }      
}
endBatch{
    if(duplicateCheckFlag == true){
        debug(collAdtMaintainQuery);
        int i = sqlPrepUpdate(collAdtMaintainQuery,(long)mapGet(mpAudit,\"CHECKSUM\"),(string)mapGet(mpAudit,\"WF_NAME\"),DB);
         debug(\"Rows inserted:\"+i);   
